%{
// Add a virtual semicolon to avoid conflicting.
open System
open FSharp.HTML
open FSharp.HTML.HtmlTokenUtils

type token = HtmlToken
%}

last =CDATA|COMMENT|TAGEND|TAGSELFCLOSING|TEXT
first=CDATA|COMMENT|TAGSELFCLOSING|TAGSTART|TEXT
any = <last> | <first>

%%

<last> / <first> { [lexbuf.Head;SEMICOLON] }
EOF              { [] }
<any>            { lexbuf }