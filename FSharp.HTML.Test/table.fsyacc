%{
// for optional </tr>
%}

table: 
    | "<table>" children "</table>"  {}

children:
    | caption colgroup thead tbody tfoot tr thead tbody tfoot tr {}

caption:
    | (* empty *) {}
    | "<caption>" "</caption>" {}
    | "<caption/>"             {}

colgroup:
    | (* empty *)                {}
    | "<colgroup>" "</colgroup>" {}
    | "<colgroup/>"  {}

thead:
    | (* empty *)          {}
    | "<thead>" tr "</thead>" {}

    | "<thead/>" {}

tfoot:
    | (* empty *)          {}
    | "<tfoot>" tr "</tfoot>" {}

    | "<tfoot/>" {}

tbody:
    | (* empty *)   {}
    | "<tbody>" tr "</tbody>" {}
    | "<tbody/>" {}

tr:
    | (* empty *)   {}
    | "<tr>" "</tr>" {}
    | "<tr/>" {}

